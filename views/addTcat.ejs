    <script>
        // Create stars for background
        document.addEventListener('DOMContentLoaded', function() {
            const starsContainer = document.getElementById('stars');
            const starsCount = 100;
            
            for (let i = 0; i < starsCount; i++) {
                const star = document.createElement('div');
                star.classList.add('star');
                
                // Random position
                const x = Math.random() * 100;
                const y = Math.random() * 100;
                
                // Random size
                const size = Math.random() * 2 + 1;
                
                // Random animation duration and delay
                const duration = Math.random() * 3 + 2;
                const delay = Math.random() * 5;
                
                star.style.left = `${x}%`;
                star.style.top = `${y}%`;
                star.style.width = `${size}px`;
                star.style.height = `${size}px`;
                star.style.setProperty('--duration', `${duration}s`);
                star.style.setProperty('--delay', `${delay}s`);
                
                starsContainer.appendChild(star);
            }
            
            // Toggle settings sidebar on mobile
            const settingCollapseBtn = document.querySelector('.setting-collpase-btn');
            const settingSidebar = document.querySelector('.setting-sidebar');
            
            if (settingCollapseBtn && settingSidebar) {
                settingCollapseBtn.addEventListener('click', function() {
                    if (settingSidebar.style.display === 'block') {
                        settingSidebar.style.display = 'none';
                        settingCollapseBtn.textContent = 'Show Settings Menu';
                    } else {
                        settingSidebar.style.display = 'block';
                        settingCollapseBtn.textContent = 'Hide Settings Menu';
                    }
                });
            }
        });

        function populatePositions() {
            $.ajax({
                type: 'GET',
                url: '/positions',
                success: function (positions) {
                    const selectBox = $('#input-position');
                    selectBox.empty(); // Clear existing options

                    positions.forEach(function (position) {
                        if (typeof position === 'undefined') {
                            // Option 1: Display specific text for undefined
                            selectBox.append('<option value="">Undefined</option>');
                        } else {
                            // Option 2: Create option with position value (including undefined)
                            selectBox.append(`<option value="${position}">${position}</option>`);
                        }
                    });
                },
                error: function (xhr, status, error) {
                    console.error('Error fetching positions:', error);
                    // Handle error
                }
            });
        }

        // Call the function to populate positions when the page is loaded
        $(function () {
            populatePositions();
        });
    </script>
</body>

</html>